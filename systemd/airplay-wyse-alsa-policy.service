[Unit]
Description=AirPlay Wyse ALSA Policy Ensure
Documentation=https://github.com/robertmayen/airplay_wyse
Wants=network-online.target
After=network-online.target airplay-wyse-audio-kmods.service
Before=shairport-sync.service
ConditionPathIsExecutable=/usr/local/libexec/airplay_wyse/alsa-policy-ensure

[Service]
Type=oneshot
ExecStart=/usr/local/libexec/airplay_wyse/alsa-policy-ensure
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
